{
  "credential_issuer": "${external-url}",
  "credential_endpoint": "${external-url}/api/v1/credential",
  "credential_response_encryption": {
    "encryption_required": false,
    "alg_values_supported": [
      "RSA-OAEP-256",
      "ECDH-ES+A128KW"
    ],
    "enc_values_supported": [
      "A128CBC-HS256"
    ]
  },
  "version": "1.0",
  "display": [
    {
      "name": "SilentByte Aussteller",
      "locale": "de-CH",
      "logo": {
        "uri": "data:image/png;base64,"
      }
    },
    {
      "name": "SilentByte Issuer",
      "locale": "en-US",
      "logo": {
        "uri": "data:image/png;base64,"
      }
    }
  ],
  "credential_configurations_supported": {
    "my_credential_sd_jwt": {
      "format": "vc+sd-jwt",
      "credential_signing_alg_values_supported": [
        "ES256"
      ],
      "vct": "my_credential_sd_jwt",
      "display": [
        {
          "name": "Whoami certificate",
          "locale": "de-CH",
          "logo": {
            "uri": "data:image/png;base64,"
          },
          "description": "Demo Identity",
          "background_color": "#AB47BC"
        }
      ],
      "claims": {
        "lastName": {
          "mandatory": true,
          "value_type": "string",
          "display": [
            {
              "locale": "de-CH",
              "name": "Last Name"
            }
          ]
        },
        "firstName": {
          "mandatory": true,
          "value_type": "string",
          "display": [
            {
              "locale": "de-CH",
              "name": "First Name"
            }
          ]
        },
        "address": {
          "type": "object",
          "properties": {
            "street_address": {
              "type": "string"
            },
            "locality": {
              "type": "string"
            },
            "region": {
              "type": "string"
            },
            "country": {
              "type": "string"
            }
          }
        }
      }
    },
    "member_card_sd_jwt": {
      "format": "vc+sd-jwt",
      "credential_signing_alg_values_supported": [
        "ES256"
      ],
      "vct": "member_card_sd_jwt",
      "display": [
        {
          "name": "Member certificate",
          "locale": "de-CH",
          "logo": {
            "uri": "data:image/png;base64,"
          },
          "description": "Demo Member",
          "background_color": "#FF9800"
        }
      ],
      "claims": {
        "type": {
          "mandatory": true,
          "value_type": "string",
          "display": [
            {
              "locale": "de-CH",
              "name": "Member Type"
            }
          ]
        },
        "since": {
          "mandatory": true,
          "value_type": "string",
          "display": [
            {
              "locale": "de-CH",
              "name": "Since"
            }
          ]
        }
      }
    },
    "university_example_sd_jwt": {
      "format": "vc+sd-jwt",
      "credential_signing_alg_values_supported": [
        "ES256"
      ],
      "cryptographic_binding_methods_supported": [
        "did:jwk"
      ],
      "proof_types_supported": {
        "jwt": {
          "proof_signing_alg_values_supported": [
            "ES256"
          ]
        }
      },
      "vct": "university_example_sd_jwt",
      "claims": {
        "degree": {
          "type": {
            "mandatory": true,
            "value_type": "string",
            "display": [
              {
                "locale": "de-CH",
                "name": "Abschluss Typ"
              }
            ]
          },
          "name": {
            "mandatory": true,
            "value_type": "string",
            "display": [
              {
                "locale": "de-CH",
                "name": "Diplomtitle"
              }
            ]
          },
          "average_grade": {
            "mandatory": false,
            "value_type": "number",
            "display": [
              {
                "locale": "de-CH",
                "name": "Notendurchschnitt"
              }
            ]
          }
        }
      }
    },
    "unbound_example_sd_jwt": {
      "format": "vc+sd-jwt",
      "credential_signing_alg_values_supported": [
        "ES256"
      ],
      "vct": "unbound_example_sd_jwt",
      "claims": {
        "animal": {
          "mandatory": true,
          "value_type": "string",
          "display": [
            {
              "locale": "de-CH",
              "name": "Tier"
            }
          ]
        }
      }
    }
  }
}
