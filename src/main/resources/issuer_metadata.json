{
  "credential_issuer": "${external-url}",
  "credential_endpoint": "${external-url}/credential",
  "credential_response_encryption": {
    "encryption_required": false,
    "alg_values_supported": [
      "RSA-OAEP-256",
      "ECDH-ES+A128KW"
    ],
    "enc_values_supported": [
      "A128CBC-HS256"
    ]
  },
  "version": "1.0",
  "display": [
    {
      "name": "Strassenverkehrsamt Appenzell Ausserrhoden",
      "locale": "de-CH",
      "logo": {
        "uri": "data:image/png;base64,"
      }
    },
    {
      "name": "Road traffic office Appenzell Ausserrhoden",
      "locale": "en-US",
      "logo": {
        "uri": "data:image/png;base64,"
      }
    }
  ],
  "credential_configurations_supported": {
    "${stage}university_example_sd_jwt": {
      "format": "vc+sd-jwt",
      "credential_signing_alg_values_supported": [
        "ES256"
      ],
      "proof_types_supported": {
        "jwt": {
          "proof_signing_alg_values_supported": [
            "ES256",
            "ES512"
          ]
        }
      },
      "display": [
        {
          "name": "${stage}Beispiel Credential",
          "locale": "de-CH",
          "logo": {
            "uri": "data:image/png;base64,"
          },
          "description": "Beispiel Credential",
          "background_color": "#007AFF"
        },
        {
          "name": "${stage}Example Credential",
          "locale": "en-US",
          "logo": {
            "uri": "data:image/png;base64,"
          },
          "description": "Test Credential",
          "background_color": "#007AFF"
        }
      ],
      "vct": "${stage}_university_example_sd_jwt",
      "claims": {
        "degree": {
          "type": {
            "mandatory": true,
            "value_type": "string",
            "display": [
              {
                "locale": "de-CH",
                "name": "Abschluss Typ"
              }
            ]
          },
          "name": {
            "mandatory": true,
            "value_type": "string",
            "display": [
              {
                "locale": "de-CH",
                "name": "Diplomtitle"
              }
            ]
          },
          "average_grade": {
            "mandatory": false,
            "value_type": "number",
            "display": [
              {
                "locale": "de-CH",
                "name": "Notendurchschnitt"
              }
            ]
          }
        }
      }
    },
    "university_example_sd_jwt": {
      "format": "vc+sd-jwt",
      "credential_signing_alg_values_supported": [
        "ES256"
      ],
      "proof_types_supported": {
        "jwt": {
          "proof_signing_alg_values_supported": [
            "ES256",
            "ES512"
          ]
        }
      },
      "vct": "university_example_sd_jwt",
      "claims": {
        "degree": {
          "type": {
            "mandatory": true,
            "value_type": "string",
            "display": [
              {
                "locale": "de-CH",
                "name": "Abschluss Typ"
              }
            ]
          },
          "name": {
            "mandatory": true,
            "value_type": "string",
            "display": [
              {
                "locale": "de-CH",
                "name": "Diplomtitle"
              }
            ]
          },
          "average_grade": {
            "mandatory": false,
            "value_type": "number",
            "display": [
              {
                "locale": "de-CH",
                "name": "Notendurchschnitt"
              }
            ]
          }
        }
      }
    },
    "unbound_example_sd_jwt": {
      "format": "vc+sd-jwt",
      "credential_signing_alg_values_supported": [
        "ES256"
      ],
      "cryptographic_binding_methods_supported": [
        "did:tdw"
      ],
      "vct": "unbound_example_sd_jwt",
      "claims": {
        "animal": {
          "mandatory": true,
          "value_type": "string",
          "display": [
            {
              "locale": "de-CH",
              "name": "Tier"
            }
          ]
        }
      }
    }
  }
}
